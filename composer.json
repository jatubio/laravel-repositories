{
    "name": "reynholm/laravel-repositories",
    "description": "Provides an easy way to create your laravel repositories",
    "authors": [
        {
            "name": "Carlos Goce",
            "email": "carlosgoce@gmail.com",
            "homepage": "http://www.carlosgoce.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.*",
        "illuminate/database": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.3",
        "mockery/mockery": "0.9.1",
        "phpspec/prophecy-phpunit": "~1.0",
        "codeception/specify": "dev-master",
        "codeception/verify": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Reynholm\\LaravelRepositories": "src/"
        }
    },
    "minimum-stability": "stable"
}
